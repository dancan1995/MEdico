{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": [
      "es2017"
    ],
    "rootDir": "src",
    "outDir": "lib",
    "types": [
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ],
  "extends": "expo/tsconfig.base"
}
