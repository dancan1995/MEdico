rules_version = '2';
service cloud.firestore {
  match /databases/{db}/documents {
    match /users/{uid}/caregiverChats/{phone}/messages/{msgId} {
      allow read, write: if request.auth.uid == uid;
    }
    match /users/{uid}/{document=**} {
      allow read, write: if request.auth.uid == uid;
    }
  }
}
